    "@metaplex-foundation/js": "^0.19.4",
    "convex": "^1.2.1",
    "framer-motion": "^10.13.0",
    "is-svg": "^5.0.0",
    "openai": "^4.7.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.4",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.16.0",
    "variant": "dev"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.1",
    "@types/node": "^18.16.19",
    "@types/react": "^18.2.15",
