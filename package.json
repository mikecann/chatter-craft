{
  "name": "chatter-craft",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm-run-all dev:init --parallel dev:server dev:client",
    "build": "tsc && vite build",
    "dev:server": "convex dev",
    "dev:client": "npm-run-all --parallel dev:bundler dev:types",
